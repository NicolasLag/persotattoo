<h1> Upload your proposal </h1>

<div class="well">
  <div class="container-fluid">
    <div class="col-xs-12 col-md-6 col-md-offset-3">

    <%= simple_form_for [@project, @proposal] do |f| %>
      <%= f.input :photos, label: false, as: :attachinary %>
      <%= f.input :content, placeholder: 'explain why you decided to draft these awesome drafts' %>
      <%= f.button :submit, class: "btn btn-success btn-block"%>
    <% end %>

    </div>
  </div>
</div>


